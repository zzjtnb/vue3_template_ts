{
  "name": "vue3-template",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@6.32.3",
  "engines": {
    "node": ">= 16",
    "pnpm": ">= 6"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "prepare": "husky install",
    "lint:eslint": "eslint --cache --cache-location .cache/eslint/ --max-warnings 0  \"src/**/*.{vue,ts}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.(js|json|css|scss|vue|html)\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.(vue|scss|css)\" --cache --cache-location .cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./config/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint": "pnpm lint:lint-staged && pnpm lint:pretty",
    "preinstall": "node ./config/preinstall.js"
  },
  "dependencies": {
    "pinia": "^2.0.12",
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.0-beta.34",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.1",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss-html": "^1.3.0",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.6.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.9",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  }
}
