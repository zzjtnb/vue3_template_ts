{
  "name": "vue-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install",
    "eslint": "eslint \"src/**/*.{vue,js}\" --cache --cache-location .cache/eslint/",
    "eslint:fix": "eslint \"src/**/*.{vue,js}\" --fix --cache --cache-location .cache/eslint/",
    "stylelint": "stylelint \"src/**/*.(vue|scss|css)\"",
    "stylelint:fix": "stylelint \"src/**/*.(vue|scss|css)\" --fix --cache --cache-location .cache/stylelint/",
    "lint": "pnpm eslint && pnpm stylelint",
    "lint:fix": "pnpm eslint:fix && pnpm stylelint:fix",
    "lint:staged": "lint-staged -c ./config/lintstagedrc.js",
    "preinstall": "node ./config/preinstall.js"
  },
  "engines": {
    "node": ">= 16",
    "pnpm": ">= 6"
  },
  "dependencies": {
    "element-plus": "^2.1.11",
    "pinia": "^2.0.13",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/node": "^16.11.27",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "eslint": "^8.14.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "postcss-html": "^1.4.1",
    "sass": "^1.51.0",
    "stylelint": "^14.8.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7"
  }
}
